            <Image url="@../img/loginClothify.jpeg" />
         </image>
      </ImageView>
      <TextField fx:id="txtID" focusTraversable="false" layoutX="250.0" layoutY="418.0" prefHeight="49.0" prefWidth="348.0" promptText="Employee ID">

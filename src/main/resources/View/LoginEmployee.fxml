            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </TextField>
      <TextField fx:id="txtPassword" focusTraversable="false" layoutX="250.0" layoutY="509.0" prefHeight="49.0" prefWidth="348.0" promptText="password" />

<AnchorPane nodeOrientation="LEFT_TO_RIGHT" prefHeight="674.0" prefWidth="1204.0" stylesheets="@../CssFiles/EmployeeLogin.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.clothify.Controller.LoginEmployeeController">
   <children>
      <ImageView fitHeight="676.0" fitWidth="1563.0" layoutY="-3.0" nodeOrientation="RIGHT_TO_LEFT" pickOnBounds="true" preserveRatio="true">
